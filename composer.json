{
    "name": "styde/enlighten",
    "description": "Enlighten your APIs with auto-generated documentation",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^7.28|^8.0|^9.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^5.0|^6.0",
        "friendsofphp/php-cs-fixer": "^2.16|^3.1",
        "styde/enlighten-base-template": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Duilio Palacios",
            "email": "duilio@styde.net"
        },
        {
            "name": "Jeffer Ochoa",
            "email": "jeffer.8a@gmail.com"
        }
    ],
    "autoload": {
        "files": ["src/helpers.php"],
        "psr-4": {
            "Styde\\Enlighten\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Styde\\Enlighten\\Providers\\EnlightenServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url" : "../enlighten-base-template"
        }
    ]
}
